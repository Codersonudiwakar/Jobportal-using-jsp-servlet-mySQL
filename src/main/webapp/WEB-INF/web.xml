<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet1</servlet-name>
  	<display-name>LoginServlet1</display-name>
  	<description></description>
  	<servlet-class>com.servlet.LoginServlet1</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<display-name>login</display-name>
  	<description></description>
  	<servlet-class>com.servlet.login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddJobServlet</servlet-name>
  	<display-name>AddJobServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.AddJobServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet1</servlet-name>
  	<url-pattern>/LoginServlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddJobServlet</servlet-name>
  	<url-pattern>/AddJobServlet</url-pattern>
  </servlet-mapping>
</web-app>
